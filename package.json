{
  "name": "dex-arbitrage",
  "version": "1.0.0",
  "main": "index.js",
  "author": "djaciel",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint \"**/*.{js,ts}\"",
    "solhint": "solhint contracts/**/*.sol",
    "lint": "yarn eslint && yarn solhint",
    "lint:fix": "yarn eslint --fix && yarn solhint --fix",
    "prettier": "prettier \"**/*.{ts,js,sol}\"",
    "prettier:check": "yarn prettier --check",
    "prettier:fix": "yarn prettier --write",
    "compile": "hardhat compile",
    "test": "yarn clean && yarn compile && hardhat test",
    "typechain": "TS_NODE_TRANSPILE_ONLY=true hardhat typechain",
    "coverage": "hardhat coverage",
    "flatten": "hardhat flatten",
    "deploy": "hardhat deploy --network",
    "docgen": "hardhat docgen",
    "reinstall": "rm -rf node_modules/ && rm -f yarn.lock && yarn",
    "clean": "hardhat clean",
    "size": "hardhat size-contracts",
    "verify": "hardhat etherscan-verify --license AGPL-3.0 --force-license --solc-input --network",
    "start-fork": "FORK_ENABLED=true hardhat node --no-reset --no-deploy",
    "find": "hardhat run scripts/find.ts --network",
    "call": "hardhat run scripts/call.ts --network",
    "getPairsData": "hardhat run scripts/getPairsData.ts --network",
    "getPairsPaths": "hardhat run scripts/getPairsPaths.ts --network",
    "getTokensAddress": "hardhat run scripts/getTokensAddress.ts --network",
    "performArbitrage": "hardhat run scripts/performArbitrage.ts --network",
    "performArbitrage2": "hardhat run scripts/performArbitrage2.ts --network",
    "performArbitrage3": "hardhat run scripts/performArbitrage3.ts --network",
    "performArbitrage4": "hardhat run scripts/performArbitrage4.ts --network",
    "exampleScript": "hardhat run scripts/exampleScript.ts --network",
    "getPairsDoubles": "hardhat run scripts/getPairsDoubles.ts --network"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.7.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "bignumber.js": "^9.0.2",
    "chai": "^4.3.6",
    "dotenv": "^16.0.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.9",
    "hardhat": "^2.9.9",
    "hardhat-abi-exporter": "^2.10.0",
    "hardhat-contract-sizer": "^2.6.1",
    "hardhat-deploy": "^0.11.11",
    "hardhat-docgen": "^1.3.0",
    "hardhat-gas-reporter": "^1.0.8",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.21",
    "ts-node": "^10.8.2",
    "typechain": "^8.1.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^0.27.2"
  }
}
